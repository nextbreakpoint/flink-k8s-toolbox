      <TABLE ALIGN="LEFT" BORDER="0" CELLBORDER="0" CELLSPACING="0">
          <TR>
              <TD>Job modified</TD>
          </TR>
      </TABLE>
  > ];
  STARTED -> STOPPING [ label = <
      <TABLE ALIGN="LEFT" BORDER="0" CELLBORDER="0" CELLSPACING="0">
          <TR>
              <TD>Cluster not started</TD>
          </TR>
      </TABLE>
  > ];
  STARTED -> STOPPING [ label = <
      <TABLE ALIGN="LEFT" BORDER="0" CELLBORDER="0" CELLSPACING="0">
          <TR>
              <TD>Cluster starting</TD>
          </TR>
      </TABLE>
  > ];
